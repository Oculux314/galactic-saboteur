<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import nz.ac.auckland.se206.components.AnimatedButton?>

<Pane fx:id="panBackground" prefHeight="600.0" prefWidth="800.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nz.ac.auckland.se206.controllers.TitleController">
   <children>
      <Label fx:id="lblTitle" alignment="CENTER" contentDisplay="CENTER" layoutY="227.0" prefHeight="71.0" prefWidth="800.0" text="Galactic Saboteur" textAlignment="CENTER">
         <font>
            <Font size="50.0" />
         </font>
      </Label>
      <AnimatedButton fx:id="btnSettings" fitHeight="80.0" fitWidth="80.0" layoutX="280.0" layoutY="390.0" onMouseClicked="#onSettingsClicked">
         <image>
            <Image url="@../images/settings.png" />
         </image>
      </AnimatedButton>
      <AnimatedButton fx:id="btnPlay" fitHeight="80.0" fitWidth="80.0" layoutX="440.0" layoutY="390.0" onMouseClicked="#onPlayClicked">
         <image>
            <Image url="@../images/play.png" />
         </image>
      </AnimatedButton>
   </children>
</Pane>
